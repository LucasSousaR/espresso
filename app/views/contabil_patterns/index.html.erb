<%= breadcrumb true %>

<%= index_search_form_for @q do |f| %>
  <%= f.input :id_cont, required: false, as: :string %>
<% end %>
<br>

<%= index_table_for(@items) do |t|
  t.column :id
  t.column :label, 'Nome' do |i|
    i.label
  end

  t.column :company_id, 'Empresas' do |i|
    Company.find(i.company_id).name
  end

end
%>